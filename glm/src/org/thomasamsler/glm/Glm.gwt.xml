<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">
<module rename-to='glm'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>
  
  <inherits name='com.google.gwt.gears.Gears'/>
  <inherits name='com.google.gwt.ajaxloader.AjaxLoader'/>
  <inherits name='com.google.gwt.maps.GoogleMaps'/>
  <script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=true_or_false&amp;key=ABQIAAAAhz6w4o8Dnx4MWEXjy_RcIRRyJhidimuyiD6wWJvAZS9_-Fun-BRA8GoYY4alwdSD-vASZEddXXaDDg" />
  
  --><!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.standard.Standard'/>
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='org.thomasamsler.glm.client.Glm'/>
  
  <!-- Rebind the entry point if Gears is not installed -->
  <replace-with class="org.thomasamsler.glm.client.NoGears">
    <when-type-is class="org.thomasamsler.glm.client.Glm"/>
    <when-property-is name="gears.installed" value="false"/>
  </replace-with>
</module>
